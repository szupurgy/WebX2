// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id         Int    @id @default(autoincrement())
  vezeteknev String
  keresztnev String
  email      String @unique
  jelszo     String
  telszam    String
  szuldatum  String

  @@map("Felhasználók")
}

model termek {
  id     Int    @id @default(autoincrement())
  nev    String
  leiras String
  ar     Int

  @@map("Termékek")
}

model cim {
  id           Int    @id @default(autoincrement())
  orszag       String
  varos        String
  utca         String
  hazszam      Int
  iranyitoszam Int

  @@map("SzalCimek")
}

model Fmod {
  id    Int    @id @default(autoincrement())
  tipus String

  @@map("Fizetésimódok")
}

model Szmod {
  id    Int    @id @default(autoincrement())
  tipus String

  @@map("Szállításimódok")
}

model rendeles {
  id       Int    @id @default(autoincrement())
  datum    String
  TeljesAr Int

  @@map("rendelések")
}

model megrendelttermek {
  id        Int @id @default(autoincrement())
  mennyiseg Int
  ar        Int

  @@map("Megrendelések")
}
